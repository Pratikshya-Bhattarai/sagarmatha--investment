# Sagarmatha Investments - Environment Variables Template
# Copy this file to .env.local and fill in your actual values

# Google Analytics (Optional)
NEXT_PUBLIC_GA_MEASUREMENT_ID=your-ga-measurement-id

# Supabase Configuration
NEXT_PUBLIC_SUPABASE_URL=your-supabase-project-url
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-supabase-anon-key
SUPABASE_SERVICE_ROLE_KEY=your-supabase-service-role-key

# Backend API Configuration
NEXT_PUBLIC_API_URL=https://your-backend.pythonanywhere.com/api/v1
BACKEND_API_URL=https://your-backend.pythonanywhere.com/api/v1

# NEPSE Data Configuration
NEPSE_DATA_SOURCE=kaggle
KAGGLE_USERNAME=your-kaggle-username
KAGGLE_KEY=your-kaggle-api-key

# Vercel Analytics (Automatic)
# Vercel Analytics and Speed Insights work automatically on Vercel

# Development Configuration
NODE_ENV=production
NEXT_PUBLIC_APP_URL=https://sagarmatha-investments.vercel.app
